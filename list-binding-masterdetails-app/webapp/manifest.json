{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.sap.listbindingmasterdetailsapp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.15.7",
            "toolsId": "38df85cd-3992-41e1-88a4-a5c5b69da2d7"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.131.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.sap.listbindingmasterdetailsapp.i18n.i18n"
                }
            },
            "":{
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/fruits.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "idAppCon",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "com.sap.listbindingmasterdetailsapp.view",
                "async": true,
                "viewPath": "com.sap.listbindingmasterdetailsapp.view"
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": "",
                    "target": [
                        "TargetView1", "EmptyView"
                    ]
                },
                {
                    "name": "RouteView2",
                    "pattern": "View2/{fruitId}",
                    "target": [
                        "TargetView2", "TargetView1"
                    ]
                },
                {
                    "name": "RouteView3",
                    "pattern": "View3/{suppId}",
                    "target": [
                        "TargetView3", "TargetView1"
                    ]
                }
            ],
            "targets": {
                "TargetView1": {
                    "id": "View1",
                    "name": "View1",
                    "controlAggregation": "masterPages"
                },
                "EmptyView": {
                    "id": "Empty",
                    "name": "Empty",
                    "controlAggregation": "detailPages"
                },
                "TargetView2": {
                    "id": "View2",
                    "name": "View2",
                    "controlAggregation": "detailPages"
                },
                "TargetView3": {
                    "id": "Supplier",
                    "name": "Supplier",
                    "controlAggregation": "detailPages"
                }
            }
        },
        "rootView": {
            "viewName": "com.sap.listbindingmasterdetailsapp.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}